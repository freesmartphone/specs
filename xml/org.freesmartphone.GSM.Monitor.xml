<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/">
  <interface name="org.freesmartphone.GSM.Monitor">
    <method name="GetServingCellInformation">
      <annotation name="org.freedesktop.DBus.GLib.Async" value="true"/>
      <arg type="a{sv}" name="serving_cell" direction="out"/>
      <fso:throws xmlns:fso="http://www.freesmartphone.org/schemas/DBusSpecExtension" type="org.freesmartphone.GSM.Device"/>
    </method>
    <method name="GetNeighbourCellInformation">
      <annotation name="org.freedesktop.DBus.GLib.Async" value="true"/>
      <arg type="a(a{sv})" name="neighbour_cells" direction="out"/>
      <fso:throws xmlns:fso="http://www.freesmartphone.org/schemas/DBusSpecExtension" type="org.freesmartphone.GSM.Device"/>
    </method>
  </interface>
</node>
