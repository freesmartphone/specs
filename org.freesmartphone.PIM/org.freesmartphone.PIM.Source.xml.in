<?xml version="1.0"?>
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="freesmartphone-pim-source-interface">

<doc:doc>
  <doc:summary>freesmartphone.org PIM.Source Interface</doc:summary>
</doc:doc>

<interface name="org.freesmartphone.PIM.Source">

  <doc:doc>
    <doc:para>
      Get information about one PIM data source 
    </doc:para>
  </doc:doc>

  <method name="GetName">
    <annotation name="org.freedesktop.DBus.GLib.Async" value="fso_pim_source" />
    <doc:doc>
      <doc:description>Get the sources name.</doc:description>
    </doc:doc>
    <arg type="s" name="source_name" direction="out">
        <doc:doc>
            <doc:summary>The name of the PIM data source</doc:summary>
        </doc:doc>
    </arg>
  </method>

  <method name="GetStatus">
    <annotation name="org.freedesktop.DBus.GLib.Async" value="fso_pim_source" />
    <doc:doc>
      <doc:description>Get the sources status.</doc:description>
    </doc:doc>
    <arg type="s" name="source_status" direction="out">
        <doc:doc>
            <doc:summary>The status of the PIM data source</doc:summary>
        </doc:doc>
    </arg>
  </method>

  <method name="GetSupportedPIMDomains">
    <annotation name="org.freedesktop.DBus.GLib.Async" value="fso_pim_source" />
    <doc:doc>
      <doc:description>Get the list of PIM domains this source supports.</doc:description>
    </doc:doc>
    <arg type="as" name="source_domains" direction="out">
        <doc:doc>
            <doc:summary>The list of supported PIM domains</doc:summary>
        </doc:doc>
    </arg>
  </method>

<!-- leads to a name collission in vala-dbus-binding-tool (free_smartphone_pim_source_init also used as type init)

  <method name="Init">
    <doc:doc>
      <doc:description>Init backend source.</doc:description>
    </doc:doc>
  </method>

-->

  <method name="Enable">
    <doc:doc>
      <doc:description>Enable backend.</doc:description>
    </doc:doc>
  </method>

  <method name="Disable">
    <doc:doc>
      <doc:description>Disable backend.</doc:description>
    </doc:doc>
  </method>

  <method name="GetEnabled">
    <doc:doc>
      <doc:description>Check, if backend is enabled.</doc:description>
    </doc:doc>
    <arg type="b" name="source_enabled" direction="out">
        <doc:doc>
            <doc:summary>Current state of backend (enabled or disabled)</doc:summary>
        </doc:doc>
    </arg>
  </method>

  <method name="GetProperties">
    <doc:doc>
      <doc:description>Get the list of properties this source supports. Possible properties: add_entry, upd_entry, del_entry, upd_entry_with_new_field, needs_login</doc:description>
    </doc:doc>
    <arg type="as" name="source_properties" direction="out">
        <doc:doc>
            <doc:summary>The list of supported properties</doc:summary>
        </doc:doc>
    </arg>
  </method>

  <method name="SetAsDefault">
    <doc:doc>
      <doc:description>Set this backend as default in specified domain.</doc:description>
    </doc:doc>
    <arg type="s" name="domain" direction="in">
        <doc:doc>
            <doc:summary>Name of domain</doc:summary>
        </doc:doc>
    </arg>
  </method>


</interface>

</node>
