<?xml version="1.0"?>
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="freesmartphone-phone-interface">

<doc:doc>
  <doc:summary>freesmartphone.org PIM.Sources Interface</doc:summary>
</doc:doc>

<interface name="org.freesmartphone.PIM.Sources">

  <doc:doc>
    <doc:para>
      This interface gives access to PIM Sources.
    </doc:para>
  </doc:doc>

  <method name="InitAllEntries">
    <doc:doc>
      <doc:description>
        Initialize all backend sources.
      </doc:description>
    </doc:doc>
  </method>

  <method name="GetEntryCount">
    <doc:doc>
      <doc:description>
        Get the number of sources available.
      </doc:description>
    </doc:doc>
    <arg type="i" name="count" direction="out">
      <doc:doc>
        <doc:summary>
          Amount of sources.
        </doc:summary>
      </doc:doc>
    </arg>
  </method>

</interface>

</node>

