<?xml version="1.0"?>
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd" doc:id="freesmartphone-pim-sources-interface">

<doc:doc>
  <doc:summary>freesmartphone.org PIM.Sources Interface</doc:summary>
</doc:doc>

<interface name="org.freesmartphone.PIM.Sources">

  <doc:doc>
    <doc:para>
      This interface gives access to PIM Sources.
    </doc:para>
  </doc:doc>

  <method name="InitAllEntries">
    <annotation name="org.freedesktop.DBus.GLib.Async" value="true" />
    <doc:doc>
      <doc:description>
        Initialize all backend sources.
      </doc:description>
    </doc:doc>
  </method>

  <method name="GetEntryCount">
    <annotation name="org.freedesktop.DBus.GLib.Async" value="true" />
    <doc:doc>
      <doc:description>
        Get the number of sources available.
      </doc:description>
    </doc:doc>
    <arg type="i" name="count" direction="out">
      <doc:doc>
        <doc:summary>
          Amount of sources.
        </doc:summary>
      </doc:doc>
    </arg>
  </method>

  <method name="GetDomains">
    <doc:doc>
      <doc:description>
        Get the list of PIM domains.
      </doc:description>
    </doc:doc>
    <arg type="as" name="domains" direction="out">
      <doc:doc>
        <doc:summary>
          List of available PIM domains.
        </doc:summary>
      </doc:doc>
    </arg>
  </method>

  <method name="GetDefaultBackend">
    <doc:doc>
      <doc:description>
        Get the default backend for specified domain.
      </doc:description>
    </doc:doc>
    <arg type="s" name="domain" direction="in">
      <doc:doc>
        <doc:summary>
          Name of domain.
        </doc:summary>
      </doc:doc>
    </arg>
    <arg type="s" name="backend" direction="out">
      <doc:doc>
        <doc:summary>
          Name of default backend for given domain.
        </doc:summary>
      </doc:doc>
    </arg>
  </method>

  <method name="GetBackends">
    <doc:doc>
      <doc:description>
        Get list of available backends.
      </doc:description>
    </doc:doc>
    <arg type="as" name="backends" direction="out">
      <doc:doc>
        <doc:summary>
          List of backends.
        </doc:summary>
      </doc:doc>
    </arg>
  </method>


</interface>

</node>

